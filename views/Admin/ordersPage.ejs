<%-include ('header')%>

<div class="page-body">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-12">
                <div class="card card-table">
                    <div class="card-body">
                        <div class="title-header option-title d-sm-flex d-block">
                            <h5>Orders List</h5>
                            <div class="right-options">
                                <!-- <ul>
                                   
                                    <li>
                                        <a class="btn btn-solid" href="">Add Product</a>
                                    </li>
                                </ul> -->
                            </div>
                        </div>
                        <div>
                            <div class="table-responsive">
                                <table class="table all-package theme-table table-product" id="table_id">
                                    <thead>
                                        <tr>
                                            <th>Date</th> 
                                            <th>User Name</th>
                                            <th>status</th>
                                            <th>Payment</th>
                                            <th>Payment status</th>
                                            
                                            <th>Details</th>
                                            <th>Grandtotal</th>
                                            
                                          

                                           

                                            
                                        </tr>
                                    </thead>

                                    <tbody>
                                        <%if (orders.length > 0){%>
                                            <% for (let i=0;i < orders.length; i++){ %>
                                        <tr>
                                           

                                            <td><%= orders[i].createdOn %></td>
                                       
                                            <td><%= orders[i].user.name %></td>

                                
                                            <td><%= orders[i].status %></td>


                                           <td><%= orders[i].paymentMethod %></td>

                                            <td><%= orders[i].paymentStatus %></td>

                                            <td><a href="/admin/orderDetailsPage/<%= orders[i]._id %>"
                                                class="btn-small d-block">View</a></td>
                                            <td > â‚¹<%= orders[i].totalprice %></td>
                                            
                                        </tr>

                                        <%}%>
                                        <%}%>
                                        
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    

    

</div>
<!-- page-wrapper End-->
<% if (locals.totalPages) { %>
    <!-- Bootstrap Pagination -->
    <nav aria-label="Page navigation" style="margin-bottom: 60px;size:px ;">
        <ul class="pagination justify-content-center">
            <% if (locals.currentPage> 1) { %>
                <li class="page-item">
                    <a class="page-link btn-dark" href="?page=<%= locals.currentPage - 1 %>" aria-label="Previous">
                        <span aria-hidden="true">&laquo;</span>
                    </a>
                </li>
                <% } else { %>
                    <li class="page-item disabled">
                        <span class="page-link btn-dark" aria-label="Previous">
                            <span aria-hidden="true">&laquo;</span>
                        </span>
                    </li>
                    <% } %>

                        <% for (let i=1; i <=locals.totalPages; i++) { %>
                            <li class="page-item <%= i === locals.currentPage ? 'active' : '' %>">
                                <a class="page-link btn-dark" href="?page=<%= i %>">
                                    <%= i %>
                                </a>
                            </li>
                            <% } %>

                                <% if (locals.currentPage < locals.totalPages){ %>
                                    <li class="page-item">
                                        <a class="page-link btn" href="?page=<%= locals.currentPage + 1 %>"
                                            aria-label="Next">
                                            <span aria-hidden="true">&raquo;</span>
                                        </a>
                                    </li>
                                    <% } else { %>
                                        <li class="page-item disabled">
                                            <span class="page-link btn" aria-label="Next">
                                                <span aria-hidden="true">&raquo;</span>
                                            </span>
                                        </li>
                                        <% } %>
        </ul>
    </nav>
    <% } %>



<!-- Modal Start -->
<div class="modal fade" id="staticBackdrop" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1"
aria-labelledby="staticBackdropLabel" aria-hidden="true">
<div class="modal-dialog  modal-dialog-centered">
<div class="modal-content">
    <div class="modal-body">
        <h5 class="modal-title" id="staticBackdropLabel">Logging Out</h5>
        <p>Are you sure you want to log out?</p>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>

        <div class="button-box">
            <button type="button" class="btn btn--no" data-bs-dismiss="modal">No</button>
            <button type="button" class="btn  btn--yes btn-primary">Yes</button>
        </div>
    </div>
</div>
</div>
</div>
<!-- Modal End -->

<!-- Delete Modal Box Start -->
